<?xml version="1.0" encoding="UTF-8"?>
<flow:FlowDefinition oss_common:lifeCycle="PRELIMINARY"
	oss_common:name="HelloWorldFlow" oss_common:ns="OSS_MED"
	oss_common:version="1.0.0" synchronicity="SYNCHRONOUS"
	xmlns:flow="urn:com:ericsson:schema:xml:oss:fbp_flow" xmlns:oss_common="urn:com:ericsson:schema:xml:oss:oss_common"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<oss_common:modelCreationInfo author="edejket"
		creationDate="15/10/2013">
		<oss_common:designedModel componentId="helloworld-flow-model" />
	</oss_common:modelCreationInfo>

	<oss_common:desc>This is one simple hello world flow</oss_common:desc>

	<flow:input name="helloWorldFlowInput" />

	<flow:step name="HelloWorldHandler">
		<flow:handlerTypeRef uri="model://OSS_MED/HelloWorldHandler/1.0.0" />
	</flow:step>


	<flow:path>
		<flow:from uri="direct:helloWorldFlowInput" />
		<flow:to uri="HelloWorldHandler" />
	</flow:path>
</flow:FlowDefinition>